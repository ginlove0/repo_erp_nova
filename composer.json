{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "alexbowers/nova-prepopulate-searchable": "^1.0",
        "ali-awwad/create-relation-on-resource": "^0.0.1",
        "beyondcode/nova-custom-dashboard-card": "^0.0.1",
        "bissolli/nova-phone-field": "^0.0.4",
        "digitalcloud/nova-custom-views": "^1.7",
        "eminiarts/nova-tabs": "^1.2",
        "epartment/nova-dependency-container": "^1.2",
        "erp-ipsupply/model": "*",
        "erp-ipsupply/quantity-base-condition": "*",
        "fideloper/proxy": "^4.0",
        "fourstacks/nova-repeatable-fields": "^1.3",
        "inspheric/nova-defaultable": "^1.2",
        "inspheric/nova-email-field": "^1.42",
        "inspheric/nova-indicator-field": "^1.43",
        "inspheric/nova-url-field": "^1.4",
        "ipsupply/checkout-item-resource-tool": "*",
        "ipsupply/creatable-hasmany-relation-field": "*",
        "ipsupply/create-packages-shipping": "*",
        "ipsupply/item-qty-base-condition": "*",
        "ipsupply/relation-dependance-field": "*",
        "kabbouchi/nova-logs-tool": "^0.1.4",
        "khalin/nova-link-field": "^1.2",
        "kirschbaum-development/nova-inline-relationship": "^0.2.0",
        "klepak/nova-router-link": "^1.0",
        "laravel/cashier": "^10.6",
        "laravel/framework": "^6.2",
        "laravel/nova": "*",
        "laravel/passport": "^8.4",
        "laravel/tinker": "^2.0",
        "manmohanjit/nova-belongs-to-dependency": "^1.1",
        "orlyapps/nova-belongsto-depend": "^2.0",
        "titasgailius/search-relations": "^1.0",
        "whitecube/nova-flexible-content": "^0.1.15",
        "yassi/nova-nested-form": "^3.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/QuantityBaseCondition"
        },
        {
            "type": "path",
            "url": "./nova-components/Model"
        },
        {
            "type": "path",
            "url": "./nova-components/ItemQtyBaseCondition"
        },
        {
            "type": "path",
            "url": "./nova-components/RelationDependanceField"
        },
        {
            "type": "path",
            "url": "./nova-components/CheckoutItemResourceTool"
        },
        {
            "type": "path",
            "url": "./nova-components/CreatableHasmanyRelationField"
        },
        {
            "type": "path",
            "url": "./nova-components/SaleOrderModelsField"
        },
        {
            "type": "path",
            "url": "./nova-components/CreatePackagesShipping"
        }
    ]
}
